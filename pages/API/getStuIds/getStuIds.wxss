/* pages/API/getStuIds/getStuIds.wxss */
.container {
  width:100%;
  overflow:hidden;
}
.content{
	width:94%;
	padding:0 3%;
	overflow:hidden;
}
.img-list2{
    width: 100%;
}
.img-list2 .ul .li{
    position: relative;
    width: 100%;
    height: 145px;
    overflow: hidden;
    box-shadow:0px 0px 10px 1px #ddd;
    margin-top:15px;
    background-color: #787d7b;
    color:#fff;
    border-radius: 10rpx;
}
.img-list2 .ul .li .title{
    width: 94%;
    padding:0 3%;
    padding-top:10px;
    height: 25px;
    font-size: 18px;
}
.img-list2 .ul .li .info{
    width: 94%;
    padding:0 3%;
    padding-bottom:10px;
}
.img-list2 .ul .li .info text{
    display: block;
    width: 100%;
    height: 20px;
    line-height: 20px;
}
.img-list2 .ul .li .start text{
    float:left;
}
.img-list2 .ul .li .start image{
    width: 15px;
    height: 15px;
    margin-top:12px;
    margin-left:1px;
    float:left;
}
.img-list2 .ul .li .start{
    width: 94%;
    padding:0 3%;
    height: 40px;
    line-height: 40px;
    background-color: #fff;
    color:#999;
}
.img-list2 .ul .li .image{
    position: absolute;
    width: 115px;
    height: 115px;
    right:5%;
    top:15px;
    overflow: hidden;
    border-radius: 50%;
    box-shadow:0px -1px 10px 1px #707c74;
}