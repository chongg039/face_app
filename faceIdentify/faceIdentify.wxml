<!--faceIdentify/faceIdentify.wxml-->
<form bindsubmit="formSubmit" bindreset="formReset">
  <view class="section">
    <view class="section__title">输入分组id（全校）：</view>
    <input name="input" placeholder="please input here" auto-focus/>
  </view>
  <text>识别结果：</text>
  <view wx:for="{{identify_result}}" wx:for-index="key" wx:for-item="value">
    {{key}} : {{value}}
  </view>
  <view class="btn-area">
    <button formType="submit">Submit</button>
    <button formType="reset">Reset</button>
  </view>
</form>
